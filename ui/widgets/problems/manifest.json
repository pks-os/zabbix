{
	"manifest_version": 2.0,
	"id": "problems",
	"type": "widget",
	"name": "Problems",
	"namespace": "Problems",
	"version": "1.0",
	"author": "Zabbix",
	"description": "Displays currently open problems with quick access links to the problem details.",
	"url": "https://www.zabbix.com/documentation/7.2/en/manual/web_interface/frontend_sections/dashboards/widgets/problems",
	"widget": {
		"js_class": "CWidgetProblems",
		"in": {
			"hostids": {
				"type": "_hostids"
			},
			"groupids": {
				"type": "_hostgroupids"
			}
		},
		"out": [
			{
				"type": "_eventid"
			}
		]
	},
	"actions": {
		"widget.problems.view": {
			"class": "WidgetView"
		}
	},
	"assets": {
		"js": ["class.widget.js"]
	}
}
